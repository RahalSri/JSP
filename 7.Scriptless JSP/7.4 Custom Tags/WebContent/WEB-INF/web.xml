<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Custom Tags</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <!-- Actully no need below taglib configuration. Container will automatically search for tld fiels in WEB-INF, sub-dir of WEB-INF, META-INF and sub-dir of META-INF  -->
  <jsp-config>
  	<taglib>
  		<taglib-uri>randomThings</taglib-uri>
  		<taglib-location>/WEB-INF/tlds/mytags.tld</taglib-location>
  	</taglib>
  </jsp-config>
</web-app>